{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "jsx": "react",
        "lib": ["es2015", "es2017", "dom"],
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "noImplicitThis": false
    },
    "files": [
        "./app/index.tsx"
    ]
}